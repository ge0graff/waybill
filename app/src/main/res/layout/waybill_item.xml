<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/waybill_items_constraint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:drawable/editbox_background"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/wb_rcv_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/wb_rcv_mileage"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/wb_rcv_mileage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="24dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/wb_rcv_data"
            app:layout_constraintEnd_toStartOf="@+id/wb_rcv_daly_mileage"
            app:layout_constraintStart_toEndOf="@+id/wb_rcv_data"
            app:layout_constraintTop_toTopOf="@+id/wb_rcv_data" />

        <TextView
            android:id="@+id/wb_rcv_daly_mileage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/wb_rcv_mileage"
            app:layout_constraintEnd_toStartOf="@+id/wb_rcv_fuel_value"
            app:layout_constraintStart_toEndOf="@+id/wb_rcv_mileage"
            app:layout_constraintTop_toTopOf="@+id/wb_rcv_mileage" />

        <TextView
            android:id="@+id/wb_rcv_fuel_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/wb_rcv_daly_mileage"
            app:layout_constraintEnd_toStartOf="@+id/wb_rcv_refuling"
            app:layout_constraintStart_toEndOf="@+id/wb_rcv_daly_mileage"
            app:layout_constraintTop_toTopOf="@+id/wb_rcv_daly_mileage" />

        <TextView
            android:id="@+id/wb_rcv_refuling"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/wb_rcv_fuel_value"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/wb_rcv_fuel_value"
            app:layout_constraintTop_toTopOf="@+id/wb_rcv_fuel_value" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>